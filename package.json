{
  "name": "ng-mdb",
  "version": "5.0.5",
  "description": "Bootstrap Material Design for Angular",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdbootstrap/Angular-Bootstrap-with-Material-Design",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "demo": "webpack-dev-server --config tools/config/webpack/webpack.dev.js",
    "lint": "tslint --project tslint.json src/**/*.ts -t verbose && stylelint src/**/*.scss",
    "release": "standard-version",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0",
    "@angular/forms": ">=4.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/language-service": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.53",
    "camelcase": "^4.0.0",
    "codelyzer": "^4.0.1",
    "core-js": "^2.4.1",
    "glob": "^7.1.1",
    "google-closure-compiler": "^20171112.0.0",
    "jest": "^21.2.1",
    "jest-preset-angular": "^4.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.51.7",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.2",
    "standard-version": "^4.0.0",
    "stylelint": "^8.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/lib/setupJest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/lib/tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>/node_modules/jest-preset-angular/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|/src\\/lib\\/.*\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "html"
    ],
    "moduleNameMapper": {
      "app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx)"
    ]
  }
}
